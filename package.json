    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0",
